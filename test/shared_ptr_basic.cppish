class Calculator {
  let x;
  let y;
  Calculator (this, x1, y1){
    this.x = x1;
    this.y = y1;
  }

  add(this){
    return this.x + this.y;
  }
};

main() {
    let a = 1; {
        let shared_ptr<Calculator> obj2 = new Calculator(3, 4); {
            a = a + obj2.add();
        }
        return a;
    }
}
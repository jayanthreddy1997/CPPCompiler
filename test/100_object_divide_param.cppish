class Calculator {
  Calculator(this) {}

  divide(this, x, y){
    if(y > 0){
      return x/y;
    }else{
      return -1;
    }
  }
};

main() {
  let Calculator obj = new Calculator(); {
    let a = obj.divide(10, 5); {
      let b = obj.divide(10, 0); {
        let c = a*b; {
          return c;
        }
      }
    }
  } 
}